/**index.wxss**/
.setting-header{
    font-size:56rpx;
    color:rgba(0,0,0,0.84);
    box-shadow: 0 1rpx 0 0 rgb(232,232,232);
    height:100rpx;
    line-height:100rpx;
    padding-left:30rpx;
    font-weight:bold;
}
.setting-main{

}
.content-wrap{
    display:flex;
    flex-direction:column;
    justify-content:center;
    height:118rpx;
    position:relative;
}
.content-wrap button{
    top:0;
    left:0;
    right:0;
    background-color:transparent;
    bottom:0;
    position:absolute;
}
.content{
    display:flex;
    width:100%;
    height:58rpx;
    line-height:58rpx;
    color:rgba(0,0,0,0.84);
    margin:0 30rpx;
    font-size:32rpx;
}
.content image{
    width:58rpx;
    height:58rpx;
    margin-right:30rpx;
}
.content-border{
    border: 1px solid rgb(244,244,244);
}