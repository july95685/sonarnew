page{
  background-color: #F4F4F4;
}

.profile-header{
  height: 277rpx;
  background-color: white;
  display: flex;
  align-items: center;
  justify-content: center;
  background-size: 100%;
  background-image: url('data:image/png;base64,');
}
.profile-header image{
  width: 192rpx;
	height: 192rpx;
	box-shadow: 0 5px 10px 0 rgba(0, 0, 0, 0.2);
  border: 8rpx solid white;
  border-radius: 104rpx;
  background-color: white;
}
.profile-title{
  height: 112rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 36rpx;
	font-weight: 600;
	color: rgba(0, 0, 0, 0.84);
  position: relative;
  background-color: white;
  text-align: center;
}
.profile-tabs{
  height: 88rpx;
  background-color: white;
}
.profile-tabs view{
  height: 88rpx;
  display: flex;
  flex-direction: row;
  background-color: white;
  transform: translate3d(0,0,0);
}
.profile-tabs view.fixed{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 1;
  box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.2);
}
.profile-tabs view view{
  font-size: 28rpx;
	font-weight: 500;
	color: rgba(0, 0, 0, 0.54);
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 240rpx;
  position: relative;
}
.profile-tabs view view.active{
  color: #108ee9;
}
.profile-tabs view view.active::after{
  width: 240rpx;
	height: 6rpx;
	background-color: #108ee9;
  position: absolute;
  bottom: 0;
  left: 0;
  content: "";
}
.simple-card{
  background-color: white;
  padding: 30rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
}
.simple-card > image{
  flex-shrink: 0;
  width: 160rpx;
	height: 160rpx;
	border-radius: 4rpx;
	background-color: #ffffff;
	box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.2);
}
.simple-card > view{
  display: flex;
  flex-direction: column;
  padding-left: 30rpx;
  align-content: center;
  box-sizing: border-box;
  flex: 1;
}
.simple-card__title{
	font-size: 36rpx;
	color: rgba(0, 0, 0, 0.84);
  line-height: 50rpx;
}
.simple-card__subtitle{
  font-size: 24rpx;
	height: 48rpx;
  line-height: 2;
	color: rgba(0, 0, 0, 0.54);
  margin-top: 8rpx;
}
.simple-card__tags{
  font-size: 0;
  margin-top: 6rpx; 
}
.tag-item{
  border-radius: 4rpx;
	border: solid 1px #e8e8e8;
  padding:0 10rpx;
  height: 44rpx;
  line-height: 44rpx;
  display: inline-block;
  font-size: 24rpx;
	font-weight: 500;
  color: rgba(0, 0, 0, 0.24);
  margin-right: 10rpx;
}
.tag-item text{
  /* vertical-align: middle; */
}
.tag-item text[class^=icon]{
  margin-right: 10rpx;
}

.scroll-container{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: red;
}