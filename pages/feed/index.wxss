/**index.wxss**/
.feed-headerContainer{
     margin:30rpx 30rpx 0 30rpx ;
     display:flex;
     justify-content:space-between;
}
.feed-header{
    display:flex;
    justify-content:flex-start;
}
.feed-headerIcon{
    display:flex;
    justify-content:space-between;
}
.header-left{
    width:68rpx;
    height:68rpx;
    display:flex;
    position:relative;
}
.header-fill{
    width:34rpx;
    height:68rpx;
    background-color:rgb(248,248,248);
    right:0;
    position:absolute;
}
.header-image{
    width:68rpx;
    height:68rpx;
    position:absolute;
    z-index:2;
}
.header-image image{
    width:68rpx;
    height:68rpx;
    border-radius:50%;
}
.container-image{
    width:128rpx;
    display:flex;
    flex-direction:column;
}
.container-image image{
    width:68rpx;
    height:68rpx;
    padding:30rpx;
}
.header-content{
    background-color:rgb(248,248,248);
    font-size:28rpx;
    display:flex;
    justify-content:center;
    line-height:68rpx;
    height:68rpx;
    padding:0 20rpx;
}
.header-content .icon-down{
    margin-top:22rpx;
    margin-left:10rpx;
}
.header-right{
    width:34rpx;
    height:68rpx;
    border-radius:0 100px 100px 0;
    background-color:rgb(248,248,248);
}
.blue-point{
    width:16rpx;
    height:16rpx;
    background:rgb(16,142,233);
    margin:0 auto;
    border-radius:50%;
}
.feedOrData{
    display:flex;
    flex-direction:row;
    justify-content:flex-start;
    margin-top:50rpx;
    margin-left:30rpx;
    font-size:40rpx;
    font-weight:bold;
}
.gruyColor{
    color:rgba(0,0,0,0.24);
}
.feed-toolbar{
    margin-top:32rpx;
    display:flex;
    justify-content:space-between;
    padding:0 30rpx;
    height:88rpx;
    line-height:88rpx;
    box-shadow: 0 1rpx 0 0 rgb(232,232,232);
}
.feed-toolbar icon{
    width: 32rpx;
    height: 32rpx;
}
.tool-choose{
    border-bottom:6rpx solid rgb(16,142,233);
    color:rgb(16,142,233);
}
.tool-unchoose{
    border-bottom:6rpx solid rgb(16,142,233);
    color:rgb(16,142,233);
}
.tool-content{
    display:flex;
}
.tool-setup{
    margin-right:30rpx;
}
.tool-setup icon{
    width: 32rpx;
    height: 42rpx;
}
.feed-container{
    display:flex;
    min-height:220rpx;
    box-shadow: 0 1rpx 0 0 rgb(244,244,244);
}
.container-main{
    width:100%;
    display:flex;
    flex-direction:column;
    padding-top:30rpx;
    padding-right:30rpx;
    padding-bottom:24rpx;
}
.main-from{
    display:flex;
    justify-content:space-between;
    font-size:24rpx;
    line-height:30rpx;
}
.main-tag{
    color:rgba(0,0,0,0.54);
}
.main-time{
    color:rgba(0,0,0,0.34);
}
.main-title{
    font-size:30rpx;
    line-height:42rpx;
    color:rgba(0,0,0,0.84);
    margin:4rpx 0;
}
.main-content{
    font-size:32rpx;
    line-height:44rpx;
    color:rgba(0,0,0,0.84);
}
.data-scope{
    height:68rpx;
    line-height:68rpx;
    padding:46rpx 30rpx;
    display:flex;
}
.data-scope text{
    color:rgba(0,0,0,0.34);
    font-size:28rpx;
}
.data-rectangle{
    display:flex;
}
