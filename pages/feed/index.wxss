.space{
    height:20rpx;
    width:100%;
    background-color:rgb(232,232,232);
}
.data-title{
    color:#000;
    font-size:32rpx;
    margin-top:30rpx;
    margin-left:20rpx;
}
.data-totle{
    color:#000;
    font-size:72rpx;
    margin:20rpx 0 26rpx 20rpx;
}
.chart-title{
    margin:30rpx 20rpx;
    font-size:32rpx;
    font-color:#000;
    height:44rpx;
    line-height:44rpx;
}
.chart-row{
    margin-left:20rpx;
    margin-right:20rpx;
}
.chart-group{
    display:flex;
    flex-direction:row;
    justify-content:space-between;
    margin-bottom:30rpx;
}
.ring-info{
    width:50%;
    height:320rpx;
    display:flex;
    flex-direction:column;
    justify-content:center;

}
.ringInfo-show{
    width:100%;
    height:30rpx;
}
.ringInfo-show .ringInfo-top{
    display:flex;
    flex-direction:row;
    justify-content:top;
}
.ringInfo-show .ringInfo-top view{
    width:20rpx;
    margin:5rpx 20rpx;
    height:20rpx;
    background-color:#333333;
}
.ringInfo-show .ringInfo-bottom{
    display:flex;
    flex-direction:row;
    justify-content:top;
}
.ringInfo-show .ringInfo-bottom view{
    margin:5rpx 20rpx;
    width:20rpx;
    height:20rpx;
    background-color:#333333;
}
.ringInfo-show text{
    font-size:20rpx;
    line-height:30rpx;
}
.canvas {
    height:100%;
    width:100%;
}
.tips{
    position:absolute;
    top:0;
    bottom:0;
    left:0;
    right:0;
    background-color:rgba(0,0,0,0.54);
    z-index:999;
    display:flex;
    flex-direction:column;
    justify-content:center
}
.tips-content{
    width:520rpx;
    height:348rpx;
    background:#fff;
    margin:0 auto;

}
.tips-mian{
    width:100%;
    height:250rpx;
    display:flex;
    flex-direction:column;
    justify-content:center
}
.tips-move{
    height:92rpx;
    line-height:92rpx;
    width:100%;
    font-weight:bold;
    text-align:center;
    font-size:32rpx;
    color:rgb(16,142,233);
    border-top:1px solid rgb(232,232,232);
}
.tips-num{
    text-align:center;
    font-size:48rpx;
    color:rgb(16,142,233);
    margin:16rpx 0;
}
.tips-v2{
    position:absolute;
    top:0;
    bottom:0;
    left:0;
    right:0;
    z-index:999;
    display:flex;
    justify-content:center
}
.tips-v2-text{
    background-color:rgb(16,142,233);
    height:60rpx;
    line-height:60rpx;
    font-size:26rpx;
    color:#fff;
    position:relative;
    top:220rpx;
    border-radius:30rpx;
    width:270rpx;
    margin:0 10rpx;
    text-align:center;
}
