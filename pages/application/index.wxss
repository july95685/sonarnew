/**index.wxss**/
.application-header{
    font-size:56rpx;
    color:rgba(0,0,0,0.84);
    height:100rpx;
    line-height:100rpx;
    padding-left:30rpx;
    font-weight:bold;
}
.application-info{
    font-size:32rpx;
    color:rgba(0,0,0,0.84);
    height:100rpx;
    line-height:100rpx;
    padding-left:30rpx;
    font-weight:bold;
}
.application-info text{
    color:rgb(68,183,83);
    font-size:36rpx;
}
.form{
    margin:0 30rpx;
}
.section{
    width:100%;
    height:90rpx;
    background-color:rgb(244,244,244);
    border-radius:8rpx;
    margin-bottom:30rpx;
}
.section input{
    font-size:28rpx;
    height:90rpx;
    line-height:90rpx;
    padding-left:30rpx;
}
.phcolor{
    font-size:28rpx;
    height:90rpx;
    line-height:90rpx;
    color:rgba(0,0,0,0.34);
    font-weight:bold;
}
.subimt-btn{
    width:100%;
    height:90rpx;
    border-radius:8rpx;
    margin-top:30rpx;
}
.subimt-btn button{
    background-color:rgb(16,142,233);
    font-size:32rpx;
    line-height:90rpx;
    color:#fff;
}